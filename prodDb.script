CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ACCOUNT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,ANIMAL_ID BIGINT NOT NULL,DEADFLG BOOLEAN NOT NULL,EMAIL VARCHAR(255) NOT NULL,ESA INTEGER NOT NULL,HP INTEGER NOT NULL,LOG VARCHAR(255) NOT NULL,LOST INTEGER NOT NULL,NAME VARCHAR(40) NOT NULL,PASSWD VARCHAR(40) NOT NULL,WON INTEGER NOT NULL,CONSTRAINT SYS_CT_46 UNIQUE(NAME))
CREATE MEMORY TABLE ANIMAL(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,IMG VARCHAR(255) NOT NULL,NAME VARCHAR(255) NOT NULL)
CREATE MEMORY TABLE AUTHORITY(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,AUTHORITY VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_52 UNIQUE(AUTHORITY))
CREATE MEMORY TABLE AUTHORITY_PEOPLE(ACCOUNT_ID BIGINT NOT NULL,AUTHORITY_ID BIGINT NOT NULL,PRIMARY KEY(AUTHORITY_ID,ACCOUNT_ID),CONSTRAINT FK4C07070B56982A4D FOREIGN KEY(AUTHORITY_ID) REFERENCES AUTHORITY(ID),CONSTRAINT FK4C07070BB6A38ACD FOREIGN KEY(ACCOUNT_ID) REFERENCES ACCOUNT(ID))
CREATE MEMORY TABLE BATTLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,ANIMAL_ID BIGINT NOT NULL,REVISE INTEGER NOT NULL,TO_ID INTEGER NOT NULL,CONSTRAINT FKACC850B88A429A7 FOREIGN KEY(ANIMAL_ID) REFERENCES ANIMAL(ID))
CREATE MEMORY TABLE MAP(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,OBJECT VARCHAR(255) NOT NULL,OBJECT_ID INTEGER NOT NULL,X INTEGER NOT NULL,Y INTEGER NOT NULL)
CREATE MEMORY TABLE REQUESTMAP(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,CONFIG_ATTRIBUTE VARCHAR(255) NOT NULL,URL VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_62 UNIQUE(URL))
ALTER TABLE ACCOUNT ADD CONSTRAINT FKB9D38A2D8A429A7 FOREIGN KEY(ANIMAL_ID) REFERENCES ANIMAL(ID)
ALTER TABLE ACCOUNT ALTER COLUMN ID RESTART WITH 10
ALTER TABLE ANIMAL ALTER COLUMN ID RESTART WITH 6
ALTER TABLE AUTHORITY ALTER COLUMN ID RESTART WITH 3
ALTER TABLE BATTLE ALTER COLUMN ID RESTART WITH 1
ALTER TABLE MAP ALTER COLUMN ID RESTART WITH 26
ALTER TABLE REQUESTMAP ALTER COLUMN ID RESTART WITH 3
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
